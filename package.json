{
  "name": "mee-local-network",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "cd dist && node ./index.js",
    "build": "npm run build:client && npm run build:server",
    "deploy": "npx tsx ./src/scripts/deploy.ts",
    "ssh": "ssh rs -t \"cd mee-local-network; bash --login\"",
    "ssh:docker-restart": "ssh rs -t \"cd mee-local-network; docker compose restart\"",
    "import:mail": " npx tsx ./import/run/runImportMail.ts",
    "import:line": " npx tsx ./import/run/runImportLINE.ts",
    "tsc": "npx tsc",
    "docker:up": "docker-compose up",
    "docker:build": "docker-compose build",
    "build:client": "vite build --mode client",
    "build:server": "vite build"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "highlight.js": "^11.10.0",
    "hono": "^4.4.11",
    "html-react-parser": "^5.1.10",
    "jotai": "^2.9.3",
    "marked": "^13.0.2",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-icons": "^5.0.1",
    "react-select": "^5.8.0"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.12.2",
    "@hono/vite-ssg": "^0.1.0",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.14.9",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "mailparser": "^3.7.1",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-device-detect": "^2.2.3",
    "react-hotkeys-hook": "^4.5.0",
    "react-router-dom": "^6.24.1",
    "sass": "^1.75.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^4.0.0",
    "vite-plugin-node": "^3.1.0",
    "vite-tsconfig-paths": "^4.3.2",
    "zustand": "^4.5.4"
  }
}
